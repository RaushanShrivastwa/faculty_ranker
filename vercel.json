{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },

    {
      "src": "/(static/.*)",
      "dest": "/client/$1"
    },
    {
      "src": "/(asset-manifest\\.json|favicon\\.ico|logo192\\.png|logo512\\.png|manifest\\.json|robots\\.txt|google_icon\\.svg|images/.*)",
      "dest": "/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
